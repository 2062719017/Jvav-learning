
public class Main {

	public static void main(String[] args) {
		String str1 = "ABCD";
		String str2 = "";// 空串
		String str3 = null;
		//char为两个字节（有别于C++一字节）
		System.out.println("ABCD\"");//  \转义
		 //1_000_000==1000000(可加下划线增加阅读性)
		/**
		 * int v1=1_000_000
		 * int v2=0xFF_EC_DE_5E
		 * int v3=0b11010010_00000000_00000000_11111111
		 * double
		 * long
		 */
		int age=10;
		System.out.println(age);//syso enter
		
		/**
		 *  >>>  -
		 */
		int a=10+8;
		System.out.println(a);
		String name ="Qwist_zz"
				double height=1.74;
		System.out.println("My name is "+name+",age is "+age+",heght is "+height);
	}

}
