
public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String str1 = "ABCD";
		String str2 = "";// 空串
		String str3 = null;
		//char为两个字节（有别于C++一字节）
		System.out.println("ABCD\"");//  \转义
		 //1_000_000==1000000(可加下划线增加阅读性)
		/**
		 * int v1=1_000_000
		 * int v2=0xFF_EC_DE_5E
		 * int v3=0b11010010_00000000_00000000_11111111
		 * double
		 * long
		 */
		//alt +shift +x +j运行
		int b1=10;
		System.out.println(b1);//syso enter
		
		/**
		 *  >>>  -
		 */
		int age=10+8;
		System.out.println(age);
		String name ="Qwist_zz";
		double height=1.74;
		System.out.println("My name is "+name+",age is "+age+",heght is "+height);
		//+号拼接
		
		
		/**
		 * 位运算符号>>  >>>
		 * >>	有符号     符号补齐
		 * >>>  无符号     0补齐
		 * -128 =11111111_11111111_11111111_00000000
		 * 
		 */
		System.out.println(-128>>2);
		System.out.println(-128>>>2);
		 
		
	}

}
